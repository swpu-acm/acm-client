{
  "gitSiteUrl": "https://github.com/swpu-acm/algohub.git/",
  "changeTags": {
    "feat": "New Features",
    "fix": "Bug Fixes",
    "hotfix": "Hot Fixes",
    "perf": "Performance Improvements",
    "chore": "Chores",
    "refactor": "Refactors"
  },
  "pkgManagers": {
    "rust": {
      "version": true,
      "getPublishedVersion": "pnpm --silent version:check",
      "publish": ["pnpm version:bump --version \"${ pkgFile.version }\""]
    }
  },
  "packages": {
    "algohub": {
      "path": "./src-tauri",
      "manager": "rust"
    }
  }
}
